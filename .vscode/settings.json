// Place your settings in this file to overwrite default and user settings.
{
  "editor.folding": true,
  "files.associations": {
    "*.ejs": "html"
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/.vscode": true,
    "**/.gitignore": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": true
  }
}